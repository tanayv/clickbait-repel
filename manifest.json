{
  "manifest_version": 2,
  "name": "Clickbait-Repel",
  "description": "This extension adds a button to flag content on Twitter",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "js": ["clickbait.js"],
      "css": ["clickbait.css"]
    }
  ],
  "permissions": [
    "tabs", "http://www.google.com/*"
  ]
}